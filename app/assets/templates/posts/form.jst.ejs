<h2>Submit a Post</h2>

<form id="post-form">

  <label for="title">Title</label>
  <input
    id="title"
    type="text"
    name="post[title]"
    value="<%= title %>">
 <br>

 <label for="body">Body</label>
 <textarea id="body" name="post[body]"> <%= body %> </textarea>

 <input type="submit"></input>

</form>